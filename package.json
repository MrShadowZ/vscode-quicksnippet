{
    "name": "quicksnippet",
    "displayName": "QuickSnippet: Custom Snippets",
    "description": "Quickly create custom snippets for any language from selected text.",
    "icon": "images/running-with-scissors.png",
    "version": "0.0.1",
    "publisher": "mousetraps",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createSnippet"
    ],
    "keywords": [
        "snippets",
        "custom",
        "javascript",
        "go",
        "python",
        "C#"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createSnippet",
                "title": "Create Snippet"
            }
        ]
    },
    "bugs": {
        "url": "https://github.com/mousetraps/vscode-quicksnippet"
    },
    "homepage": "https://github.com/mousetraps/vscode-quicksnippet/blob/master/README.md",
    "devDependencies": {
        "eslint": "^1.10.1",
        "eslint-config-google": "^0.3.0",
        "vscode": "0.10.x"
    },
    "dependencies": {
        "comment-json": "^1.1.3",
        "prepend-file": "^1.3.0"
    }
}